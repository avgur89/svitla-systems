			+e.controls
				+e.NAV.nav.nav
					ul
						-navList=['About Us', 'How We Work', 'Expertise', 'Portfolio', 'Blog', 'Contacts']
						-
							navSublist = [
								['Svitla', 'CEO', 'Our team', 'CSR', 'Career'],
								['Managed Team Extension', 'Build-to-Order Software Applications'],
								['web', 'front-end', 'back-end', 'ecommers', 'Mobile', 'Quality Assurance', 'Design', 'Internet of Things', 'Big Data', 'Dev Ops', 'Tech Support', 'Database', 'web 2', 'front-end 2', 'back-end 2'],
								['Testimonials', 'Case studies'],
								[],
								[]
							]
						each el, index in navList
							li
								if index === 4 || index === 5
									a.disabled(href='https://www.google.com.ua' data-hover= el): span= el
								else
									a(href='index.html' data-hover= el): span= el
									ul(data-more-url='#!more' data-more-text='more' data-hide-index='6')
										each el2, index2 in navSublist[index]
											li
												a(href='' data-hover= el2): span= el2
				+e.search-btn
