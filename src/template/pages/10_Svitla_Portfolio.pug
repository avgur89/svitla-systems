extends ../extend/default

block variables
	-
		title = 'Portfolio',
		breadList = ['main', 'portfolio'];

block main
	main.content(data-page='testemonials')

		//begin screen
		+b.screen
			+e.img
				img(src='static/img/portfolio.jpg' alt='image')
			+e.CANVAS.canvas_center#screenCanvas
			+e.container.container
				+e.description(data-anim='group')
					+e.description-top(data-anim-stagger='fade-top' data-anim-parent-trigger data-mob-anim='group' data-mob-anim-stagger='fade-top') We deliver unparalleled value to our clients
				+e.svg-line
					svg(viewBox='0 0 600 600')
						path(d='M600,0 0,600')
				+e.svg-circle
					svg(viewBox='0 0 80 80')
						circle(cx='40' cy='40' r='30')
				+e.content
					+e.scroll.is-active(data-hover='scroll')
						span scroll
					+e.title-w
						+e.SPAN.title-bot
							+e.H1.title Portfolio
		//end screen

		//case begin
		+b.SECTION.cases
			+e.container.container(data-anim='group' data-anim-delay='0.4')
				+e.H2.title.h1 Case studies
				+e.cont
					+e.item.anim-line(data-mob-anim='group' data-mob-anim-stagger='fade-top')
						+e.A.media(href="")
							img(src='static/img/half-11.jpg' alt='image')
						+e.info
							+e.A.subtitle.h4(href="" data-anim-stagger='fade-top' data-mob-anim='group' data-mob-anim-stagger='fade-top' data-anim-parent-trigger)
